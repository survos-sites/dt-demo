{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "name": "survos/dt-demo",
    "description": "A Symfony application that demonstrations datatables and other survos bundles",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-pdo_sqlite": "*",
        "ext-redis": "*",
        "ext-sodium": "*",
        "ext-sqlite3": "*",
        "api-platform/core": "^3.2",
        "doctrine/doctrine-bundle": "^2.10.2",
        "doctrine/doctrine-migrations-bundle": "^3.2.4",
        "doctrine/orm": "^2.16.2",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "league/flysystem-aws-s3-v3": "^3.22",
        "league/flysystem-bundle": "^3.3",
        "liip/imagine-bundle": "^2.12",
        "meilisearch/meilisearch-php": "^1.5",
        "nelmio/cors-bundle": "^2.3.1",
        "nyholm/psr7": "^1.8",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.24.2",
        "presta/sitemap-bundle": "^4.0",
        "sentry/sentry-symfony": "^4.12",
        "survos/api-grid-bundle": "^1.5",
        "survos/auth-bundle": "1.5.140",
        "survos/bootstrap-bundle": "^1.5",
        "survos/command-bundle": "^1.5",
        "survos/core-bundle": "^1.5",
        "survos/crawler-bundle": "^1.5",
        "survos/deployment-bundle": "^1.5",
        "survos/html-prettify-bundle": "^1.5",
        "survos/inspection-bundle": "^1.5",
        "survos/scraper-bundle": "^1.5",
        "survos/simple-datatables-bundle": "^1.5",
        "survos/wiki-bundle": "^1.5",
        "symfony/amqp-messenger": "^6.4",
        "symfony/asset": "^7.0",
        "symfony/asset-mapper": "7.0.3",
        "symfony/console": "^7.0",
        "symfony/doctrine-messenger": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/expression-language": "^7.0",
        "symfony/flex": "^2.4.1",
        "symfony/form": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/http-client": "^6.4",
        "symfony/intl": "^7.0",
        "symfony/mailer": "^7.0",
        "symfony/mime": "^7.0",
        "symfony/monolog-bundle": "^3.8",
        "symfony/notifier": "^7.0",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/property-info": "^7.0",
        "symfony/redis-messenger": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/security-bundle": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/stimulus-bundle": "^2.x-dev",
        "symfony/string": "^7.0",
        "symfony/translation": "^7.0",
        "symfony/twig-bundle": "^7.0",
        "symfony/ux-lazy-image": "^2.13",
        "symfony/ux-twig-component": "^2.13",
        "symfony/validator": "^7.0",
        "symfony/web-link": "^7.0",
        "symfony/yaml": "^7.0",
        "twig/extra-bundle": "^2.12|^3.7.1",
        "twig/intl-extra": "^3.8",
        "twig/markdown-extra": "^3.8",
        "twig/twig": "^3.7.1",
        "zenstruck/messenger-monitor-bundle": "1.x-dev"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "endroid/installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default  \"": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4.4",
        "nunomaduro/phpinsights": "^2.10",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6.13",
        "rector/rector": "^0.19.2",
        "spaze/phpstan-disallowed-calls": "^3.0",
        "survos/maker-bundle": "^1.5",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/debug-bundle": "^7.0",
        "symfony/maker-bundle": "^1.51.1",
        "symfony/phpunit-bridge": "^6.3.6",
        "symfony/stopwatch": "^7.0",
        "symfony/web-profiler-bundle": "^7.0",
        "symplify/easy-coding-standard": "^12.0"
    },
    "repositories": {

    }
}
